page {
    background-color: #f9faf9;
}

.swiper-box {
    background-color: #FFFFFF;
}

.container-bg {
    height: 310rpx;
}

.container-bg swiper-item {
    display: flex;
    justify-content: center;
    align-items: center;
}

.member-main {
    width: 100%;
    height: 100%;
    background-size: cover;
    box-sizing: border-box;
    padding: 32rpx;
    position: relative;
    color: #FFFFFF;
    border-radius: 20rpx;
}

.member-now {
    position: absolute;
    right: 0;
    top: 0;
    background-color: #F9F9F9;
    font-size: 22rpx;
    width: 120rpx;
    height: 46rpx;
    border-radius: 0 16rpx 0 16rpx;
    text-align: center;
    line-height: 46rpx;
}

.main-level {
    font-size: 42rpx;
    font-weight: bold;
}

.main-name {
    font-size: 28rpx;
}

.slider {
    width: 100%;
    height: 8rpx;
    background-color: rgba(255,255,255,.3);
    border-radius: 100px;
    position: relative;
    overflow: hidden;
    margin: 32rpx 0;
}

.slider-value {
    position: absolute;
    height: 8rpx;
    left: 0;
    top: 0;
    background-color: #FFFFFF;
    border-radius: 100rpx;
    transition: all 0.8s;
}

.slider-bg {
    width: 100%;
    height: 8rpx;
    margin: 32rpx 0;
}

.slider-txt {
    font-size: 22rpx;
    padding-bottom: 6rpx;
    display: flex;
    align-items: center;
    position: relative;
}

.slider-arrow {
    width: 18rpx;
    height: 18rpx;
    margin-left: 8rpx;
}

.slider-next {
    display: flex;
    align-items: center;
    font-size: 22rpx;
}

.next-time {
    flex: 1;
    text-align: right;
    color: rgba(255,255,255,.7);
}

.tab-box {
    min-height: 1000rpx;
}

.member-right-box {
    padding: 32rpx;
    background-color: #FFFFFF;
}

.member-right {
    border-radius: 16rpx;
    border: 0.5px solid #F2F3F2;
    display: flex;
    align-items: center;
}

.right-block {
    flex: 1;
    display: flex;
    align-items: center;
    padding: 28rpx 24rpx;
    position: relative;
}

.right-block:after {
    content: " ";
    position: absolute;
    right: 0;
    top: 16rpx;
    bottom: 16rpx;
    width: 0.5px;
    background-color: #F2F3F2;
}

.right-block:last-child:after {
    display: none;
}

.right-icon {
    width: 60rpx;
    height: auto;
    margin-right: 8rpx;
}

.right-txt {
    font-size: 22rpx;
    color: #333333;
}

.right-txt>view:first-child {
    font-weight: bold;
}

.exclusive-box,.free-express-box,.birthday-box {
    margin: 24rpx 32rpx 0;
    background-color: #FFFFFF;
    border-radius: 16rpx;
    padding: 24rpx;
}

.head {
    width: 100%;
    height: 40rpx;
    display: flex;
    align-items: center;
    margin-bottom: 24rpx;
}

.title {
    font-size: 28rpx;
    color: #333333;
    flex: 1;
    height: 100%;
    display: flex;
    align-items: center;
    font-weight: bold;
}

.title>text {
    font-size: 20rpx;
    color: #999999;
    font-weight: normal;
    margin-left: 8rpx;
}

.more {
    height: 100%;
    font-size: 20rpx;
    color: #666666;
    position: relative;
    padding-right: 20rpx;
    display: flex;
    align-items: center;
}

.content {
    display: flex;
    align-items: center;
    justify-content: center;
    min-height: 100rpx;
    position: relative;
}

.content-block {
    width: 33.3333333%;
    box-sizing: border-box;
    padding: 0 24rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.content-block-coupon {
    width: 100%;
    position: relative;
    background-image: url("https://qcampfile.oss-cn-shanghai.aliyuncs.com/qtoolsapp/membershop/member_coupon_bg.png");
    background-repeat: no-repeat;
    background-size: 100%;
    display: flex;
    justify-content: flex-start;
    align-items: flex-start;
    color: #FFFFFF;
    font-size: 54rpx;
    box-shadow: 0 5px 10px 0 rgba(249,133,123,0.2);
}

.content-block-left {
    width: 40rpx;
    height: 88rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 18rpx;
    writing-mode: vertical-lr;
    letter-spacing: 4rpx;
    margin-right: 2rpx;
}

.content-block-right {
    flex: 1;
    height: 88rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.coupon-money {
    text-align: center;
}

.coupon-money>text {
    font-size: 20rpx;
}

.coupon-rule {
    font-size: 24rpx;
    color: #333333;
    margin-top: 24rpx;
    font-weight: bold;
}

.coupon-time {
    font-size: 20rpx;
    color: #999999;
}

.coupon-button {
    height: 40rpx;
    line-height: 40rpx;
    border-width: 0.5px;
    border-style: solid;
    border-radius: 26rpx;
    text-align: center;
    font-size: 20rpx;
    margin-top: 16rpx;
    padding: 0 16rpx;
}

.no-receive {
    border-color: #F0344F;
    color: #F0344F;
}

.has-receive {
    border-color: #999999;
    color: #999999;
}

.content-img {
    width: 100%;
    height: auto;
}

.free-express-txt {
    position: absolute;
    left: 32rpx;
    top: 0;
    bottom: 0;
    font-size: 32rpx;
    color: #FFFFFF;
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.foot {
    display: flex;
    align-items: center;
    justify-content: flex-end;
    font-size: 20rpx;
    color: #999999;
    margin-top: 8rpx;
}

.birthday-box {
    margin-bottom: 50rpx;
}