.tips {
    width: 100%;
    height: 60rpx;
    line-height: 60rpx;
    background-color: rgba(254,240,197,0.6);
    font-family: PingFangHK-Regular;
    font-size: 24rpx;
    color: #FBC23B;
    text-align: center;
}

.form {
    display: block;
    width: 85%;
    margin: 0 auto;
}

.title {
    margin: 60rpx 0 40rpx 0;
    font-family: PingFangHK-Regular;
    font-size: 28rpx;
    color: #666666;
}

.field {
    position: relative;
    border-bottom: 1px solid #9B9B9B;
}

.complement-address {
    color: #333333;
    font-size: 32rpx;
    width: 100%;
    text-align: left;
    padding: 10rpx 0;
    box-sizing: border-box;
}

.input-placeholder {
    font-family: PingFangHK-Regular;
    font-size: 32rpx;
    color: #C9C9C9;
}

input,textarea {
    padding: 10rpx 0;
    color: #333333;
    font-size: 32rpx;
    width: 100%;
}

.len {
    position: absolute;
    right: 0;
    bottom: 13rpx;
    font-family: PingFangHK-Regular;
    font-size: 22rpx;
    color: #464646;
}

.place-holder-text {
    padding: 12px 0!important;
    color: #c9c9c9;
    font-size: 32rpx!important;
}

.cell-wrap {
    padding: 12px 0!important;
    color: #333333;
    font-size: 32rpx!important;
}

.arrow {
    position: absolute;
    right: 0;
    bottom: 13rpx;
    font-size: 30rpx;
    color: #464646;
}

.field .i-cell {
    padding: 0;
    font-size: 32rpx;
    color: #C9C9C9;
}

.btn {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    width: 100%;
    height: 110rpx;
    line-height: 110rpx;
    background-color: #464646;
    font-size: 40rpx;
    color: #E1DCC7;
    text-align: center;
    border-radius: 0;
}

.chooseConent {
    background-color: #fff;
}