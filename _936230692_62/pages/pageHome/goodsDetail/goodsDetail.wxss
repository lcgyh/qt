    font-size: 22rpx;
    margin-right: 18rpx;
    border-radius: 6rpx;
    font-family: PingFangHK-Regular;
    color: #FF7357;
    background-color: #FFEFED;
    white-space: nowrap;
}

.activity-content-text {
    display: flex;
    flex-direction: column;
    margin-bottom: 30rpx;
}

.activity-content-text-title {
    font-family: PingFang-SC-Medium;
    font-size: 24rpx;
    color: #4A4A4A;
}

.activity-content-text-list {
    margin-top: 20rpx;
}

.activity-content-text-content {
    display: flex;
    align-items: center;
    margin-bottom: 20rpx;
}

.activity-content-text-list .activity-content-text-content:last-child {
    margin-bottom: 0;
}

.activity-img {
    width: 88rpx;
    height: 88rpx;
    border: 0.5px solid #ECECEC;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: 8rpx;
    margin-right: 20rpx;
}

.activity-img>image {
    width: 96%;
    height: 96%;
}

.activity-info {
    font-family: PingFang-SC-Medium;
    font-size: 20rpx;
    color: #999999;
    flex: 1;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    text-overflow: ellipsis;
    overflow: hidden;
    word-break: break-all;
}

.activity-content-time {
    font-family: PingFang-SC-Medium;
    font-size: 20rpx;
    color: #999999;
    margin-top: 10rpx;
}

.go-activity {
    position: absolute;
    right: 0;
    top: 0;
    font-family: PingFang-SC-Medium;
    font-size: 20rpx;
    color: #999999;
    padding: 0 16rpx;
}

.go-activity::after {
    content: " ";
    display: inline-block;
    width: 4px;
    height: 4px;
    position: absolute;
    top: 50%;
    right: 1px;
    border-width: 1px 1px 0 0;
    border-color: #464646;
    border-style: solid;
    transform: translateY(-50%) matrix(.71,.71,-.71,.71,0,0);
}