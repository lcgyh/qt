    padding-right: 12px;
}

.real-name::after {
    content: " ";
    display: inline-block;
    width: 6px;
    height: 6px;
    position: absolute;
    top: 50%;
    right: 2px;
    border-width: 1px 1px 0 0;
    border-color: #464646;
    border-style: solid;
    transform: translateY(-50%) matrix(.71,.71,-.71,.71,0,0);
}

.real-name-hd {
    margin-right: 20rpx;
}

.real-name-ft {
    flex: 1;
    color: #BABABB;
    text-align: right;
}

.online-tips {
    font-family: PingFangSC-Regular;
    font-size: 20rpx;
    color: #C7C7C7;
    margin-top: 10rpx;
    text-align: justify;
    text-align-last: left;
}

.online-tips>image {
    width: 20rpx;
    height: 20rpx;
    margin-right: 6rpx;
    position: relative;
    top: 2rpx;
}

.detail-box {
    padding: 0 32rpx;
    margin-bottom: 32rpx;
}

.detail-goods {
    background-color: #FFFFFF;
    border-radius: 30rpx;
    padding: 32rpx;
}

.goods-box {
    margin-bottom: 50rpx;
}

.goods-box:last-child {
    margin-bottom: 0;
}

.detail-title {
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #464646;
    padding-bottom: 20rpx;
}

.goods-block {
    display: flex;
    margin-top: 40rpx;
    overflow: hidden;
}

.goods-block:first-child {
    margin-top: 0;
}

.goods-img {
    width: 180rpx;
    height: 180rpx;
    border: 0.5px solid #EEECEC;
    border-radius: 10rpx;
    margin-right: 20rpx;
    box-shadow: 0 10rpx 10rpx 0 #F9F9F9;
    display: flex;
    justify-content: center;
    align-items: center;
}

.goods-img image {
    width: 95%;
    height: 95%;
    border-radius: 10rpx;
}

.order-content {
    flex: 1;
    display: flex;
    flex-direction: column;
}

.goods-title {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    text-overflow: ellipsis;
    overflow: hidden;
    font-size: 24rpx;
    color: #333333;
}

.goods-tpye {
    margin-top: 10rpx;
    font-size: 24rpx;
    color: #c9c9c9;
}

.price-number {
    display: flex;
    align-items: center;
}

.price-number .member {
    width: 32rpx;
    height: auto;
}

.goods-price {
    font-family: PingFangHK-Regular;
    font-size: 28rpx;
    color: #333333;
}

.goods-number {
    flex: 1;
    height: 40rpx;
    text-align: right;
    line-height: 40rpx;
    font-family: PingFangHK-Regular;
}

.goods-number>text:first-child {
    font-size: 24rpx;
    color: #C9C9C9;
}

.goods-number>text:last-child {
    font-size: 28rpx!important;
    color: #333333!important;
}

.gift-list-box {
    padding: 0 32rpx;
    margin-bottom: 32rpx;
}

.giftList {
    padding: 32rpx;
    background-color: #FFFFFF;
    border-radius: 30rpx;
}

.giftList-title {
    padding-bottom: 32rpx;
    border-bottom: 0.5px solid #E2E2E2;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #464646;
}

.giftList-content {
    display: flex;
    align-items: center;
    padding-top: 20rpx;
}

.gift-icon {
    font-family: PingFangSC-Regular;
    font-size: 20rpx;
    color: #FD7C61;
    width: 54rpx;
    height: 28rpx;
    line-height: 28rpx;
    text-align: center;
    border: 0.5px solid #FD7C61;
    border-radius: 14rpx;
}

.gift-name {
    width: 600rpx;
    font-family: PingFangHK-Regular;
    font-size: 24rpx;
    color: #666666;
    margin: 0 20rpx 0 8rpx;
