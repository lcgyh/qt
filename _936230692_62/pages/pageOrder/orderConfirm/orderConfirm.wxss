page {
    background-color: #f8f9f8;
}

.color-line {
    position: absolute;
    bottom: 0;
    left: 32rpx;
    right: 32rpx;
}

.color-line>image {
    width: 100%;
    height: auto;
}

.anounce-tips {
    padding: 8rpx 40rpx;
    min-height: 32rpx;
    line-height: 32rpx;
    background-color: rgba(254,240,197,0.6);
    font-family: PingFangHK-Regular;
    font-size: 24rpx;
    color: #FBC23B;
    letter-spacing: 1px;
    margin-bottom: 32rpx;
}

.choose-send {
    padding: 40rpx 32rpx 20rpx;
    display: flex;
    background-color: #FFFFFF;
    margin-bottom: 32rpx;
}

.choose-send-title {
    display: flex;
    align-items: center;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #464646;
}

.choose-send-title>image {
    width: 26rpx;
    height: 26rpx;
    margin-left: 6rpx;
}

.choose-send-tab {
    flex: 1;
}

.choose-send .van-tabs--card {
    margin: 0;
    padding-top: 54rpx!important;
}

.choose-send .van-tabs__wrap {
    height: 54rpx!important;
}

.choose-send .van-tabs__scroll--card {
    border: 1px solid #00C4B3!important;
    border-radius: 200rpx!important;
    overflow: hidden!important;
}

.choose-send .van-tabs__nav--card {
    height: 54rpx!important;
}

.choose-send .van-tab {
    color: #00C4B3!important;
    font-size: 26rpx!important;
    border-right: 1px solid #00C4B3!important;
    font-family: PingFangHK-Thin;
    padding: 0 20rpx!important;
    line-height: 54rpx!important;
}

.choose-send .van-tab:last-child {
    border-right: none!important;
}

.choose-send .van-tab.van-tab--active {
    color: #FFFFFF!important;
    background-color: #00C4B3!important;
}

.send-content {
    padding: 0 32rpx;
    margin-bottom: 32rpx;
}

.tab-content {
    background-color: #FFFFFF;
    padding: 32rpx;
    font-size: 0;
    position: relative;
    margin-bottom: 32rpx;
    border-radius: 30rpx;
}

.tab-content-tips {
    width: 100%;
    height: 50rpx;
    line-height: 50rpx;
    font-family: PingFangHK-Regular;
    font-size: 22rpx;
    color: #DD8282;
    background: #FFF0E9;
    border-radius: 6rpx;
    text-align: center;
    margin-bottom: 40rpx;
}

.tab-content-info {
    padding-right: 58rpx;
    position: relative;
}

.tab-content-info::after {
    content: " ";
    display: inline-block;
    width: 6px;
    height: 6px;
    position: absolute;
    top: 50%;
    right: 2px;
    border-width: 1px 1px 0 0;
    border-color: #464646;
    border-style: solid;
    transform: translateY(-50%) matrix(.71,.71,-.71,.71,0,0);
}

.store-name,.store-time,.store-address {
    font-family: PingFangHK-Regular;
    font-size: 24rpx;
    color: #666666;
    margin-bottom: 10rpx;
}

.store-address {
    margin-bottom: 0;
}

.out-tips {
    font-family: PingFangHK-Regular;
    font-size: 28rpx;
    color: #DD8282;
    margin-bottom: 20rpx;
}

.address-content {
    font-family: PingFangHK-Regular;
    font-size: 28rpx;
}

.last-arrow {
    width: 18rpx;
    height: 18rpx;
    position: absolute;
    right: 0;
    top: 50%;
    margin-top: -9rpx;
}

.tab-content-noaddress {
    width: 100%;
    height: 100rpx;
    border: 1px dashed #c9c9c9;
    display: flex;
    justify-content: center;
    align-items: center;
    font-family: PingFangHK-Regular;
    font-size: 32rpx;
    color: #333333;
}

.tab-content-noaddress>image {
    width: 28rpx;
    height: 28rpx;
    margin: 2rpx 20rpx 0 0;
}

.tab-content-store {
    font-family: PingFangHK-Regular;
    font-size: 28rpx;
    color: #666666;
    text-align: left;
}

.tab-content-store-title {
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #464646;
    margin-bottom: 20rpx;
}

.address-box {
    margin-top: 20rpx;
}

.address-box .store-name {
    font-family: PingFangHK-Regular;
    font-size: 28rpx;
    color: #666666;
}

.express-box,.detail-summary {
    margin: 0 32rpx;
    display: flex;
    flex-direction: column;
}

.real-name-box {
    padding: 32rpx;
    background-color: #FFFFFF;
    border-radius: 30rpx;
}

.real-name {
    font-family: PingFangSC-Regular;
