    overflow: hidden;
    text-overflow: ellipsis;
}

.gift-qty {
    flex: 1;
    font-family: PingFangHK-Regular;
    font-size: 24rpx;
    color: #666666;
    text-align: right;
}

.detail-summary-box {
    padding: 32rpx;
    background-color: #FFFFFF;
    border-radius: 30rpx;
}

.detail-summary {
    margin-bottom: 32rpx;
}

.detail-summary-title {
    font-family: PingFangHK-Medium;
    font-size: 32rpx;
    color: #333333;
    margin-bottom: 40rpx;
}

.summary {
    display: flex;
}

.summary:last-child {
    margin-bottom: 0!important;
}

.summary.middle {
    margin: 30rpx 0;
