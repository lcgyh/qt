    padding-right: 20rpx;
}

.send-price {
    margin-bottom: 30rpx;
}

.send-need {
    font-family: PingFangHK-Regular;
    font-size: 22rpx;
    color: #C9C9C9;
}

.middle::after {
    content: " ";
    display: inline-block;
    width: 6px;
    height: 6px;
    position: absolute;
    top: 16rpx;
    right: 0;
    border-width: 1px 1px 0 0;
    border-color: #464646;
    border-style: solid;
    transform: translateY(-50%) matrix(.71,.71,-.71,.71,0,0);
}

.summary-title,.summary-value {
    font-size: 24rpx;
    color: #464646;
}

.nouse {
    color: #00C4B3;
}

.summary-title {
    flex: 1;
    font-family: PingFangSC-Regular;
}

.summary-value {
    font-family: PingFangHK-Medium;
    text-align: right;
}

.total .summary-title {
    font-weight: bold;
}

.total .summary-value {
    font-family: PingFangHK-Medium;
    font-size: 24rpx;
}

.freeExpress-box {
    padding: 0 32rpx;
