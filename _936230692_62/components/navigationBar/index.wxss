.menu-button {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 1500;
    width: 100%;
    height: 32px;
    background-color: #fff;
    display: flex;
    padding-bottom: 5px;
}

.menu-left-button-home {
    width: 87px;
    margin-left: 10px;
    padding: 6px 0;
    display: flex;
    border: 1px solid #f2f2f2;
    border-radius: 100px;
    box-sizing: border-box;
}

.menu-left-button {
    width: 87px;
    height: 100%;
    display: flex;
    padding-left: 10px;
    justify-content: flex-start;
    align-items: center;
    position: relative;
    z-index: 60;
    font-size: 0;
}

.menu-left-button>image {
    width: 20rpx;
    height: auto;
}

.goBack,.goHome {
    flex: 1;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 0;
}

.menu-line {
    width: 1px;
    height: 100%;
    background-color: #f2f2f2;
}

.goBack>image {
    width: 16rpx;
    height: auto;
}

.goHome>image {
    width: 32rpx;
    height: auto;
    position: relative;
    top: -1px;
}

.menu-title {
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}

.menu-title>text {
    width: 100%;
    font-size: 16px;
    text-align: center;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}