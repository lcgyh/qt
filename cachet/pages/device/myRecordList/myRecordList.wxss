.navbar {
  width: 100%;
  height: 44px;
  background: white;
  position: fixed;
  display: flex;
  justify-content: flex-start;
  border-bottom: 1rpx solid #eee;
  border-top: 1rpx solid #eee;
  text-align: center;
  /* align-items:center; */
}

.navbarItem {
  width: 16.66666%;
  height: 44px;
  text-align: center;
  line-height: 44px;
}

.listView {
  padding-top: 44px;
  width: 100%;
}

.listItem {
  width: 100vw;
  height: 72px;
  background-color: white;
  display: flex;
  border-top: 5px solid #f7f7f7;
  justify-content: space-between;
}

.item {
  height: 24px;
  line-height: 20px;
  margin-left: 10px;
}

.listItemT {
  width: 100vw;
  height: 96px;
  background-color: white;
  display: flex;
  flex-direction: column;
  border-top: 5px solid #f7f7f7;
}

.sendTime {
  display: flex;
  flex-direction: row;
}

.sendTimeText {
  margin-left: 30px;
}

.row_right {
  height: 20px;
  width: 20px;
  line-height: 72px;
  margin-right: 12px;
  margin-top: 26px;
}

.status {
  height: 24px;
  line-height: 72px;
  margin-right: 15px;

}



.empty{
  text-align: center;
  margin-top: 45vh;
  width: 100vw;
  color: gainsboro;
}