/* pages/me/me.wxss */
@import '../../../font/iconfont.wxss';

.me_top {
  background: url(data:image/png;base64,)no-repeat;
  width: 100%;
  height: 364rpx;
  background-size: 100% 100%;
  padding-top: 90rpx;
  margin-bottom: 20rpx;
  background-color: #fff;
}

.header_img {
  width: 120rpx;
  height: 120rpx;
  border: 4rpx solid #fff;
  border-radius: 50%;
  overflow: hidden;
  margin: auto auto 20rpx;
}

.bgfff {
  padding-bottom: 20rpx;
  background: #fff;
  color: #fff;
  text-align: center;
  margin-bottom: 20rpx;
}

.header_img image {
  width: 120rpx;
  height: 120rpx;
}

.bg {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.8);
}

.chose_box {
  width: 96%;
  padding: 2%;
  position: fixed;
  bottom: 0;
  left: 1%;
}

.choes_image {
  padding-left: 14px;
  padding-right: 14px;
  box-sizing: border-box;
  font-size: 18px;
  text-align: center;
  text-decoration: none;
  line-height: 2.55555556;
  border-radius: 5px;
  -webkit-tap-highlight-color: transparent;
  overflow: hidden;
  color: rgba(0, 0, 0, 0.6);
  background: #fff;
  margin-top: 20rpx;
  z-index: 9;
  position: relative;
}

.iconfont {
  font-size: 38rpx;
  margin-right: 10px;
  color: #e9221f;
}

.leftImgae {
  width: 19px;
  height: 19px;
}

.icon-tuichu {
  font-size: 48rpx;
  margin-left: -6rpx;
}

.titleText {
  line-height: 22px;
}

.unLoadView {
  position: fixed;
  top: 0;
  width: 100vw;
  height: 100vh;
  /* background: red; */
}

.qrcode {
  float: right;
  margin-right: 15px;
  margin-top: -20px;
  height: 30px;
  width: 30px;
}
