/* pages/use/inspectDetail/inspectDetail.wxss */
page{
  background: white;
}
.listItem {
  width: 100vw;
  /* height: 44px; */
  border-bottom: 1px solid #999;
  line-height: 44px;
  padding-left: 20px;
  display: flex;
  flex-wrap: wrap;
}

.item {
  height: 24px;
  line-height: 24px;
  margin-left: 10px;
}

.black {
  color: #333;
}

.listImage {
  width: 40px;
  height: 40px;
  margin-left: 10px;
  margin-top: 2px;
}

.selectBtn {
  color: white;
  background: red;
  height: 40px;
  width: 150px;
  border-radius: 5px;
  margin-top: 50px;
  line-height: 40px;
  text-align: center;
  margin-bottom: 50px;
}

.order_process {
display: flex;
flex-wrap: nowrap;
padding: 10rpx 10rpx 20rpx 10rpx;
background-color: #fff;
}
.process_wrap {
display: flex;
flex-direction: column;
flex: 1;
align-items: center;
}
.process {
display: flex;
align-items: center;
width: 100%;
}
.process_icon {
width: 50rpx;
height: 50rpx;
border-radius: 25rpx;
text-align: center;
line-height: 50rpx;
color: white;
}
.process_line {
background: #eff3f6;
flex: 1;
height: 5rpx;
}
.process_name {
font-size: 24rpx;
}
.titleText{
  margin-top: 12px;
  margin-left: 20px;
  color: #333;
}
