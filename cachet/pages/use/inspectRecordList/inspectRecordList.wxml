<!--pages/use/inspectRecordList/inspectRecordList.wxml-->
<view class='listView'>
  <view class='cengk_box' wx:for="{{array}}" wx:key="cardlist">
    <view data-devicesactid='{{item.devicesActId}}' bindtap="checkDetail" >
      <view class='listItem'>
        <view class="column">
          <text class='item' style="color:#333" decode="{{true}}">{{item.sealName}}&emsp;&emsp;&emsp;{{item.type}}</text>
          <view class='item'>申请人:{{item.name}}</view>
          <view class='item'>申请时间：{{item.takeStartTimes}}</view>
        </view>
        <view class="row">
          <view class='status' style="color:red" wx:if="{{item.censorStatusStr == '待核查'}}">{{item.censorStatusStr}}</view>
          <view class='status' wx:if="{{item.censorStatusStr != '待核查'}}">{{item.censorStatusStr}}</view>
          <image class='row_right' src='../../../images/row_right.png' mode='aspectFill'></image>
        </view>
      </view>
    </view>
  </view>
</view>