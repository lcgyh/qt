/**app.wxss**/
@import './weui-miniprogram/weui-wxss/dist/style/weui.wxss';

page {
  background: #f7f7f7;
  width: 100%;
  height: 100%;
  font-size: 26rpx;
  color: #666;
}

.font_size30 {
  font-size: 30rpx;
}

.font_size28 {
  font-size: 28rpx;
}

.font_size24 {
  font-size: 24rpx;
}

.color333 {
  color: #333;
}

.color666 {
  color: #666;
}

.color999 {
  color: #999;
}

.colorred {
  color: #e9221f;
}

.colorgreen {
  color: #00b700;
}

.colororange {
  color: #ed7000;
}

.row_right {
  background: url(data:image/png;base64,)no-repeat 700rpx center;
  background-color: #fff;
  background-size: 28rpx 45rpx;
}

.login_box {
  width: 100%;
  height: 100%;
  background: url(data:image/jpeg;base64,)no-repeat;
  background-size: 100% 100%;
  padding-top: 120rpx;
  box-sizing: border-box;
}

.loadingtips {
  font-size: 28rpx;
  text-align: center;
  padding: 50rpx;
  color: #ccc;
}

.loadingtips image, .loadingtips text {
  vertical-align: middle;
}

.loadingtips image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
}

.login_form {
  width: 100%;
  padding: 20rpx 80rpx;
  box-sizing: border-box;
}

.login_form .inputView {
  width: 100%;
  height: 80rpx;
  border: 1rpx solid #ccc;
  border-radius: 10rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}

.login_form .inputView image {
  width: 30rpx;
  height: 32rpx;
}

.login_form .inputView input {
  width: 90%;
  padding: 0 20rpx;
  box-sizing: border-box;
}

.submit {
  margin-top: 80rpx;
  text-align: center;
  line-height: 80rpx;
  height: 80rpx;
  background: #e9221f;
  color: #fff;
  font-size: 30rpx;
}

.storeman_search .content {
  display: flex;
  justify-content: space-between;
}

.storeman_search {
  background: #f7f7f7;
  padding: 30rpx 30rpx;
  box-sizing: border-box;
  width: 100%;
  border-bottom: 1rpx solid #eee;
}

.storeman_search .left {
  width: 590rpx;
  padding: 10rpx 20rpx;
  border-radius: 40rpx;
  border: 1rpx solid #eee;
  background: #fff;
  display: flex;
  align-items: center;
  height: 70rpx;
  line-height: 70rpx;
  box-sizing: border-box;
}

.storeman_search .left picker {
  margin: 0 20rpx;
}

.storeman_search .left input {
  width: 500rpx;
  padding-left: 20rpx;
}

.storeman_search image {
  width: 29rpx;
  height: 29rpx;
}

.search_btn {
  width: 80rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  text-align: center;
  margin-top: 10rpx;
  color: #999;
}

.details {
  background: #fff;
  padding-left: 30rpx;
  margin-bottom: 10rpx;
  border-bottom: 1rpx solid #dedede;
  font-size: 30rpx;
  color: #333;
  overflow: hidden;
}

.details .item {
  display: flex;
  padding: 30rpx;
  padding-left: 0rpx;
  justify-content: space-between;
  border-top: 1rpx solid #dedede;
  margin-top: -1rpx;
}

.details .noborder {
  border-bottom: none;
}

.paddingright .row_right {
  padding-right: 56rpx;
  background-position: 670rpx center;
}

.nav_block {
  display: flex;
  justify-content: space-between;
  padding: 30rpx 50rpx 30rpx 30rpx;
  width: 100%;
  box-sizing: border-box;
  background-color: #fff;
  color: #000;
  font-size: 30rpx;
  align-items: center;
  margin-bottom: 20rpx;
}

.nav_block .color999 {
  width: 500rpx;
  text-align: right;
}

.nav_block .left image {
  width: 45rpx;
  height: 45rpx;
  margin-right: 20rpx;
}

.nav_block .left, .nav_block .right {
  display: flex;
}

.nav_block .right image {
  width: 28rpx;
  height: 45rpx;
  margin-left: 10rpx;
}

.martop_4 {
  margin-top: 4rpx;
}

.storemanbtn {
  position: fixed;
  bottom: 0;
  left: 0;
  background: #fff;
  padding: 20rpx 85rpx;
  width: 100%;
  box-sizing: border-box;
}

.storemanbtn button, .storemanbtn .link {
  background: #e9221f;
  color: #fff;
  font-size: 30rpx;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
}

.storemanbtn .link {
  border-radius: 5px;
  text-align: center;
}

.padding_100 {
  padding-bottom: 100rpx;
}

.applydetails {
  background: #fff;
  padding-left: 40rpx;
  margin-bottom: 20rpx;
}

.applydetails .top {
  display: flex;
  width: 100%;
  box-sizing: border-box;
  padding: 20rpx 40rpx 20rpx 0rpx;
  border-bottom: 1rpx solid #dedede;
}

.applydetails .people {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}

.applydetails .top .state {
  width: 125rpx;
  height: 100rpx;
  margin-top: 20rpx;
}

.applydetails .top .name_box {
  display: flex;
  justify-content: center;
  flex-direction: column;
  flex: 1;
}

.applydetails .top .name_box .colororange {
  margin-top: 10rpx;
}

.top_text {
  padding: 10px 0;
  font-size: 30rpx;
}

.top_text view {
  display: flex;
  line-height: 60rpx;
}

.top_text view .left {
  width: 160rpx;
  color: #333;
}

.top_text view .right {
  width: 510rpx;
  color: #666;
}

.applydetails .data {
  display: flex;
  align-items: center;
}

.applydetails .noborder {
  border-bottom: none;
}

.applydetails .top_line {
  border-bottom: 1rpx solid #dedede;
}

.new_manage {
  display: flex;
  width: 100%;
  justify-content: space-between;
  border-bottom: 1px solid #f2f2f2;
  padding: 30rpx 0rpx;
}

.new_manage .left {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  text-align: center;
  line-height: 100rpx;
  background: #e9221f;
  color: #fff;
}

.new_manage .center {
  flex: 1;
  padding: 0 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.new_manage .right {
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.right .number {
  width: 60rpx;
  padding: 4rpx;
  height: 30rpx;
  display: inline-block;
  text-align: center;
  border: 1px solid #e9221f;
  color: #e9221f;
  border-radius: 30rpx;
  line-height: 30rpx;
}

.new_list .storemanbtn {
  border-top: 1px solid #f2f2f2;
}

.center .title {
  font-size: 30rpx;
  margin-bottom: 10rpx;
}

.scroll_box {
  box-sizing: border-box;
}

.people_list {
  display: flex;
  padding: 20rpx;
  border-bottom: 1rpx solid #f2f2f2;
}

.scroll_box .row_right {
  padding-right: 56rpx;
  background-position: 710rpx center;
}

.people_list .icon {
  width: 100rpx;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  border-radius: 50%;
  border: 1px solid #e9221f;
  color: #e9221f;
  font-size: 38rpx;
  margin-right: 20rpx;
}

.people_list .text {
  display: flex;
  flex-direction: column;
  justify-content: center;
  font-size: 30rpx;
  color: #333;
}

.people_list .text text {
  font-size: 24rpx;
  color: #999;
}

.people_list .cirler {
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  color: #fff;
  background: #e9221f;
  border-radius: 50%;
  text-align: center;
  margin-right: 20rpx;
  font-size: 30rpx;
}

.centerView {
  display: flex;
  justify-content: center;
}

.cengk_box {
  width: 100vw;
}

.shuzhe_box {
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.column {
  display: flex;
  flex-direction: column;
}

.row {
  display: flex;
  flex-direction: row;
}

.empty {
  text-align: center;
  margin-top: 45vh;
  width: 100vw;
  color: gainsboro;
}
